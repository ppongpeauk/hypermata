{
  "expo": {
    "name": "Hypermata",
    "slug": "hypermata",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "hypermata",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hoolamox.hypermata",
      "appleTeamId": "354DA25GVF",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.ppongpeauk.tramata"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to find nearby trains and stations."
        }
      ],
      "react-native-edge-to-edge",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "95b265c1-8c30-4ad2-a89e-b637a81f23af"
      }
    },
    "owner": "ppongpeauk"
  }
}
